{
  "name": "koalaw-api",
  "version": "1.0.0",
  "private": true,
  "description": "KOALAW backend API (Node + TypeScript + Express + Prisma)",
  "license": "MIT",
  "engines": {
    "node": ">=18.16"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "migrate": "tsx scripts/migrate.ts",
    "seed": "tsx scripts/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "health": "node -e \"console.log('OK')\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.601.0",
    "@aws-sdk/s3-request-presigner": "^3.601.0",
    "@elastic/elasticsearch": "^8.13.0",
    "@prisma/client": "^5.17.0",
    "bullmq": "^5.7.9",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.13",
    "pino": "^9.1.0",
    "pino-pretty": "^11.2.2",
    "stripe": "^14.25.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}